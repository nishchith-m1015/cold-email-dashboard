# E2E Testing Environment Variables
# Copy this to .env.local for E2E tests

# Clerk (use test keys or mock values)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_mock_key_for_e2e_testing
CLERK_SECRET_KEY=sk_test_mock_key_for_e2e_testing

# Supabase (use test project or mock values)
NEXT_PUBLIC_SUPABASE_URL=https://mock-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=mock-service-role-key-for-testing

# Disable telemetry in tests
NEXT_TELEMETRY_DISABLED=1

# Test mode
NODE_ENV=test
